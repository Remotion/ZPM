{
    "GitHub": {
        "host": "https://github.com/",
        "apiHost": "https://api.github.com/",
        "token": false
    },
    "install": {
        "repository": "https://github.com/Zefiros-Software/ZPM",
        "directory": "zpm",
        "modulesDirectory": "modules",
        "minReqVersion": ">5.0.0-alpha5",
        "bootstrap": {
            "repository": "https://github.com/Zefiros-Software/Bootstrap.git",
            "directory": "bootstrap"
        }  ,
        "registry": {
            "repository": "https://github.com/Zefiros-Software/Manifest.git",
            "directory": "registry",
            "directories": "registries",
            "fileName": ".registries.json",
            "modules": ".modules.json",
            "registries": ".registries.json",
            "build": ".build.json"
        },        
        "manifests": {
            "manifest": ".manifest.json",
            "buildFile": ".build.lua",
            "extensions": {
                "assets": {
                    "directory": "assets",
                    "manifest": ".assets.json",
                    "fileName": ".assets.lua",
                    "defaultExtract": false
                }
            }
        },
        "modules": {
            "fileName": ".modules.json",
            "directory": "modules"
        },
        "packages": {
            "fileName": ".package.json"
        },
        "extern": {
            "directory": "extern"  
        }
    },
    "wget": {
        "downloadUrl": "downloads.sourceforge.net/project/gnuwin32/wget/1.11.4-1/wget-1.11.4-1-setup.exe?r=http%3A%2F%2Fgnuwin32.sourceforge.net%2Fpackages%2Fwget.htm&ts=1471250872&use_mirror=netassist"
    },
    "settings": {        
    }
}